# Environment variables for n8n workflows
# This is an example file showing required environment variables
# Copy this file to .env and fill in your actual values
# IMPORTANT: Never commit the .env file to version control

# n8n Instance Configuration
N8N_INSTANCE_ID=your_n8n_instance_id_here

# Credential IDs
# These are the credential IDs from your n8n instance
# Find them in Settings > Credentials in n8n
GOOGLE_CALENDAR_ACCOUNT_CREDENTIAL_ID_4=your_google_calendar_credential_id
OPENROUTER_ACCOUNT_CREDENTIAL_ID_2=your_openrouter_credential_id
PERPLEXITY_ACCOUNT_CREDENTIAL_ID_0=your_perplexity_credential_id
POSTGRES_ACCOUNT_CREDENTIAL_ID_1=your_postgres_credential_id
REDIS_ACCOUNT_CREDENTIAL_ID_3=your_redis_credential_id
SMTP_ACCOUNT_CREDENTIAL_ID_5=your_smtp_credential_id

# Webhook IDs
# These are unique UUIDs for your n8n webhook triggers
# Generate new ones when setting up webhooks in n8n
WEBHOOK_ID_0=00000000-0000-0000-0000-000000000000
WEBHOOK_ID_1=00000000-0000-0000-0000-000000000001
WEBHOOK_ID_2=00000000-0000-0000-0000-000000000002
WEBHOOK_ID_3=00000000-0000-0000-0000-000000000003
WEBHOOK_ID_4=00000000-0000-0000-0000-000000000004

# Email Addresses
# Email addresses used in workflows for notifications and calendar events
USER_EMAIL_0=your_personal_email@example.com
USER_EMAIL_1=your_n8n_agent_email@example.com
USER_EMAIL_2=your_alternate_email@example.com

# URLs
# Service URLs used by the workflows
URL_URL_0=http://your-discord-bot-service:port/approval-request
WEBHOOKURL_URL_1=http://your-n8n-instance:5678/webhook/your-webhook-id-here
