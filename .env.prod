# =============================================================================
# PRODUCTION ENVIRONMENT TEMPLATE
# =============================================================================
# Copy this file to .env.prod and fill in your actual values
# This file contains production-specific variables that are safe to commit

# N8N configuration
N8N_INSTANCE_ID=prod_instance_retanatech
N8N_BASE_URL=https://eh-n8n.retanatech.com
N8N_EDITOR_BASE_URL=https://eh-n8n.retanatech.com
N8N_PROTOCOL=https
WEBHOOK_URL=https://eh-n8n.retanatech.com
GENERIC_TIMEZONE=America/New_York
TZ=America/New_York
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
NODE_FUNCTION_ALLOW_EXTERNAL=*
N8N_RUNNERS_ENABLED=true
N8N_PUBLIC_API_DISABLED=false
N8N_ENDPOINT_WEBHOOK=n8n

# Note: Database configuration, Redis passwords, and N8N API key
# are in .env.generated since they contain secrets or depend on generated values