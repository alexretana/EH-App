# =============================================================================
# N8N WORKFLOW CREDENTIALS TEMPLATE
# =============================================================================
# Copy this file to .env.credentials and fill in your actual values
# DO NOT commit the actual .env.credentials file to version control

# =============================================================================
# GOOGLE API CREDENTIALS
# =============================================================================
N8N_GOOGLE_CREDENTIAL_ID=your-google-credential-id-here
N8N_GOOGLE_CLIENT_ID=<YOUR_GOOGLE_CLIENT_ID_HERE>
N8N_GOOGLE_CLIENT_SECRET=<YOUR_GOOGLE_CLIENT_SECRET_HERE>
N8N_GOOGLE_ACCESS_TOKEN=<YOUR_GOOGLE_ACCESS_TOKEN_HERE>
N8N_GOOGLE_REFRESH_TOKEN=<YOUR_GOOGLE_REFRESH_TOKEN_HERE>

# =============================================================================
# SMTP EMAIL CREDENTIALS
# =============================================================================
N8N_SMTP_CREDENTIAL_ID=your-smtp-credential-id-here
N8N_SMTP_USER=<YOUR_SMTP_USERNAME_HERE>
N8N_SMTP_PASSWORD=<YOUR_SMTP_PASSWORD_HERE>
N8N_SMTP_HOST=<YOUR_SMTP_HOST_HERE>
N8N_SMTP_HOSTNAME=<YOUR_SMTP_DISPLAY_NAME_HERE>

# =============================================================================
# OPENROUTER API CREDENTIALS
# =============================================================================
N8N_OPENROUTER_CREDENTIAL_ID=your-openrouter-credential-id-here
N8N_OPENROUTER_API_KEY=<YOUR_OPENROUTER_API_KEY_HERE>

# =============================================================================
# PERPLEXITY API CREDENTIALS
# =============================================================================
N8N_PERPLEXITY_CREDENTIAL_ID=your-perplexity-credential-id-here
N8N_PERPLEXITY_API_KEY=<YOUR_PERPLEXITY_API_KEY_HERE>

# =============================================================================
# DATABASE CREDENTIALS
# =============================================================================
N8N_POSTGRES_CREDENTIAL_ID=your-postgres-credential-id-here
N8N_REDIS_CREDENTIAL_ID=your-redis-credential-id-here


# Discord bot configuration
DISCORD_BOT_TOKEN=<YOUR_DISCORD_BOT_TOKEN_HERE>
DISCORD_CHANNEL_ID=<YOUR_DISCORD_CHANNEL_ID_HERE>
