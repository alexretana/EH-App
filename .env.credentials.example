# =============================================================================
# APPLICATION CREDENTIALS TEMPLATE
# =============================================================================
# Copy this file to .env.credentials and fill in your actual values
# DO NOT commit the actual .env.credentials file to version control

# =============================================================================
# N8N INSTANCE CONFIGURATION
# =============================================================================
# Unique identifier for this n8n instance
N8N_INSTANCE_ID=your-n8n-instance-id-here

# API key for n8n endpoints
N8N_API_KEY=your-secure-n8n-api-key-here

# Base URLs for n8n instance
N8N_BASE_URL=http://localhost:5678
N8N_EDITOR_BASE_URL=http://localhost:5678

# Webhook URL for n8n workflows
WEBHOOK_URL=http://n8n:5678

# n8n Settings
GENERIC_TIMEZONE=America/New_York
TZ=America/New_York
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
NODE_FUNCTION_ALLOW_EXTERNAL=*
N8N_RUNNERS_ENABLED=true
N8N_PUBLIC_API_DISABLED=false
N8N_ENDPOINT_WEBHOOK=n8n

# =============================================================================
# DISCORD BOT CONFIGURATION
# =============================================================================
# Get these from the Discord Developer Portal
DISCORD_BOT_TOKEN=<YOUR_DISCORD_BOT_TOKEN_HERE>
DISCORD_CHANNEL_ID=<YOUR_DISCORD_CHANNEL_ID_HERE>

# =============================================================================
# N8N WORKFLOW CREDENTIAL IDs
# =============================================================================
# These are internal n8n credential IDs that reference the actual credentials
# They are generated by n8n when you set up credentials in the UI
POSTGRES_CREDENTIAL_ID=your-postgres-credential-id-here
REDIS_CREDENTIAL_ID=your-redis-credential-id-here